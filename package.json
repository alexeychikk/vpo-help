    "@types/bcryptjs": "^2.4.2",
    "@types/draft-js": "^0.11.9",
    "@types/draftjs-to-html": "^0.8.1",
    "@types/html-to-draftjs": "^1.4.0",
    "@types/lodash": "^4.14.178",
    "@types/material-ui": "^0.21.12",
    "@types/mongodb": "^4.0.7",
