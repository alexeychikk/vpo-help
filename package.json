{
  "name": "vpo-help",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/apps/api/main",
    "serve:api": "nx run api:serve",
    "serve:client": "nx run client:serve",
    "pm2:start": "env-cmd pm2 start ./dist/apps/api/main.js --name api --max-memory-restart 256M",
    "pm2:restart": "env-cmd pm2 restart api --update-env",
    "build": "npm run build:api",
    "build:api": "nx run api:build:production",
    "build:client": "nx run client:build:production",
    "build:all": "nx run-many --target=build --configuration=production --all=true",
    "test": "nx affected --target=test",
    "ts": "nx run-many --target=ts --all=true",
    "ts:affected": "nx affected --target=ts",
    "prepare": "run-p prepare:husky",
    "prepare:husky": "husky install",
    "lint:precommit": "run-s ts:affected lint:staged",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint --edit"
  },
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.11.0"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^3.0.6",
