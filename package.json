    "ts-jest": "27.0.5",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "overrides": {
    "typeorm": {
      "mongodb": "$mongodb"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,md,html,css}": [
      "prettier --write",
      "git add"
    ]
  }
